{
  "name": "ytd",
  "version": "0.0.0-a1",
  "description": "Youtube Data API CLI and library for Deno",
  "exports": "./src/mod.ts",
  "fmt": {
    "singleQuote": true
  },
  "imports": {
    "std/": "https://deno.land/std@0.224.0/",
    "cliffy/": "https://deno.land/x/cliffy@v1.0.0-rc.4/"
  },
  "tasks": {
    "lint": "deno fmt --check",
    "fmt": "deno fmt",
    "run": "deno run --allow-run --allow-env --allow-read --allow-net src/cli.ts",
    "build": "deno compile --allow-run --allow-env --allow-read --allow-net --output ytd src/cli.ts && du -h ytd",
    "test": "deno test --allow-run --allow-read --allow-write",
    "test-update": "deno test --allow-run --allow-read --allow-write -- --update",
    "lock": "deno cache --import-map=deno.json --lock-write src/mod.ts",
    "lock-reload": "deno cache --reload --lock=deno.lock deno.json"
  }
}
